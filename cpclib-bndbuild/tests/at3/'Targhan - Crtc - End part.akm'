; CRTC End part, Subsong 0.
; ----------------------------------

Subsong0
Subsong0_DisarkPointerRegionStart0
    dw Subsong0_NoteIndexes    ; Index table for the notes.
    dw Subsong0_TrackIndexes    ; Index table for the Tracks.
Subsong0_DisarkPointerRegionEnd0

Subsong0_DisarkByteRegionStart1
    db 6    ; Initial speed.

    db 1    ; Most used instrument.
    db 2    ; Second most used instrument.

    db 0    ; Most used wait.
    db 2    ; Second most used wait.

    db 48    ; Default start note in tracks.
    db 3    ; Default start instrument in tracks.
    db 26    ; Default start wait in tracks.

    db 12    ; Are there effects? 12 if yes, 13 if not. Don't ask.
Subsong0_DisarkByteRegionEnd1

; The Linker.
Subsong0_DisarkByteRegionStart2
; Position 0
    db 171    ; State byte.
    db 10    ; New speed (>0).
    db 31    ; New height.
    db 128    ; New track (0) for channel 1, as a reference (index 0).
    db ((Subsong0_Track1 - ($ + 2)) & #ff00) / 256    ; New track (1) for channel 2, as an offset. Offset MSB, then LSB.
    db ((Subsong0_Track1 - ($ + 1)) & 255)

    db 128    ; New track (0) for channel 3, as a reference (index 0).

; Position 1
    db 168    ; State byte.
    db ((Subsong0_Track2 - ($ + 2)) & #ff00) / 256    ; New track (2) for channel 1, as an offset. Offset MSB, then LSB.
    db ((Subsong0_Track2 - ($ + 1)) & 255)

    db ((Subsong0_Track3 - ($ + 2)) & #ff00) / 256    ; New track (3) for channel 2, as an offset. Offset MSB, then LSB.
    db ((Subsong0_Track3 - ($ + 1)) & 255)

    db ((Subsong0_Track4 - ($ + 2)) & #ff00) / 256    ; New track (4) for channel 3, as an offset. Offset MSB, then LSB.
    db ((Subsong0_Track4 - ($ + 1)) & 255)


; Position 2
    db 168    ; State byte.
    db ((Subsong0_Track5 - ($ + 2)) & #ff00) / 256    ; New track (5) for channel 1, as an offset. Offset MSB, then LSB.
    db ((Subsong0_Track5 - ($ + 1)) & 255)

    db ((Subsong0_Track6 - ($ + 2)) & #ff00) / 256    ; New track (6) for channel 2, as an offset. Offset MSB, then LSB.
    db ((Subsong0_Track6 - ($ + 1)) & 255)

    db ((Subsong0_Track7 - ($ + 2)) & #ff00) / 256    ; New track (7) for channel 3, as an offset. Offset MSB, then LSB.
    db ((Subsong0_Track7 - ($ + 1)) & 255)


; Position 3
    db 168    ; State byte.
    db ((Subsong0_Track8 - ($ + 2)) & #ff00) / 256    ; New track (8) for channel 1, as an offset. Offset MSB, then LSB.
    db ((Subsong0_Track8 - ($ + 1)) & 255)

    db ((Subsong0_Track9 - ($ + 2)) & #ff00) / 256    ; New track (9) for channel 2, as an offset. Offset MSB, then LSB.
    db ((Subsong0_Track9 - ($ + 1)) & 255)

    db ((Subsong0_Track10 - ($ + 2)) & #ff00) / 256    ; New track (10) for channel 3, as an offset. Offset MSB, then LSB.
    db ((Subsong0_Track10 - ($ + 1)) & 255)


; Position 4
    db 168    ; State byte.
    db ((Subsong0_Track11 - ($ + 2)) & #ff00) / 256    ; New track (11) for channel 1, as an offset. Offset MSB, then LSB.
    db ((Subsong0_Track11 - ($ + 1)) & 255)

    db ((Subsong0_Track12 - ($ + 2)) & #ff00) / 256    ; New track (12) for channel 2, as an offset. Offset MSB, then LSB.
    db ((Subsong0_Track12 - ($ + 1)) & 255)

    db ((Subsong0_Track13 - ($ + 2)) & #ff00) / 256    ; New track (13) for channel 3, as an offset. Offset MSB, then LSB.
    db ((Subsong0_Track13 - ($ + 1)) & 255)


; Position 5
    db 168    ; State byte.
    db ((Subsong0_Track14 - ($ + 2)) & #ff00) / 256    ; New track (14) for channel 1, as an offset. Offset MSB, then LSB.
    db ((Subsong0_Track14 - ($ + 1)) & 255)

    db ((Subsong0_Track15 - ($ + 2)) & #ff00) / 256    ; New track (15) for channel 2, as an offset. Offset MSB, then LSB.
    db ((Subsong0_Track15 - ($ + 1)) & 255)

    db ((Subsong0_Track16 - ($ + 2)) & #ff00) / 256    ; New track (16) for channel 3, as an offset. Offset MSB, then LSB.
    db ((Subsong0_Track16 - ($ + 1)) & 255)


; Position 6
    db 168    ; State byte.
    db ((Subsong0_Track4 - ($ + 2)) & #ff00) / 256    ; New track (4) for channel 1, as an offset. Offset MSB, then LSB.
    db ((Subsong0_Track4 - ($ + 1)) & 255)

    db ((Subsong0_Track3 - ($ + 2)) & #ff00) / 256    ; New track (3) for channel 2, as an offset. Offset MSB, then LSB.
    db ((Subsong0_Track3 - ($ + 1)) & 255)

    db ((Subsong0_Track2 - ($ + 2)) & #ff00) / 256    ; New track (2) for channel 3, as an offset. Offset MSB, then LSB.
    db ((Subsong0_Track2 - ($ + 1)) & 255)


; Position 7
    db 168    ; State byte.
    db ((Subsong0_Track5 - ($ + 2)) & #ff00) / 256    ; New track (5) for channel 1, as an offset. Offset MSB, then LSB.
    db ((Subsong0_Track5 - ($ + 1)) & 255)

    db ((Subsong0_Track6 - ($ + 2)) & #ff00) / 256    ; New track (6) for channel 2, as an offset. Offset MSB, then LSB.
    db ((Subsong0_Track6 - ($ + 1)) & 255)

    db ((Subsong0_Track7 - ($ + 2)) & #ff00) / 256    ; New track (7) for channel 3, as an offset. Offset MSB, then LSB.
    db ((Subsong0_Track7 - ($ + 1)) & 255)


; Position 8
    db 168    ; State byte.
    db ((Subsong0_Track17 - ($ + 2)) & #ff00) / 256    ; New track (17) for channel 1, as an offset. Offset MSB, then LSB.
    db ((Subsong0_Track17 - ($ + 1)) & 255)

    db ((Subsong0_Track18 - ($ + 2)) & #ff00) / 256    ; New track (18) for channel 2, as an offset. Offset MSB, then LSB.
    db ((Subsong0_Track18 - ($ + 1)) & 255)

    db ((Subsong0_Track19 - ($ + 2)) & #ff00) / 256    ; New track (19) for channel 3, as an offset. Offset MSB, then LSB.
    db ((Subsong0_Track19 - ($ + 1)) & 255)


; Position 9
    db 168    ; State byte.
    db ((Subsong0_Track20 - ($ + 2)) & #ff00) / 256    ; New track (20) for channel 1, as an offset. Offset MSB, then LSB.
    db ((Subsong0_Track20 - ($ + 1)) & 255)

    db ((Subsong0_Track21 - ($ + 2)) & #ff00) / 256    ; New track (21) for channel 2, as an offset. Offset MSB, then LSB.
    db ((Subsong0_Track21 - ($ + 1)) & 255)

    db ((Subsong0_Track22 - ($ + 2)) & #ff00) / 256    ; New track (22) for channel 3, as an offset. Offset MSB, then LSB.
    db ((Subsong0_Track22 - ($ + 1)) & 255)


; Position 10
    db 168    ; State byte.
    db ((Subsong0_Track23 - ($ + 2)) & #ff00) / 256    ; New track (23) for channel 1, as an offset. Offset MSB, then LSB.
    db ((Subsong0_Track23 - ($ + 1)) & 255)

    db ((Subsong0_Track24 - ($ + 2)) & #ff00) / 256    ; New track (24) for channel 2, as an offset. Offset MSB, then LSB.
    db ((Subsong0_Track24 - ($ + 1)) & 255)

    db ((Subsong0_Track25 - ($ + 2)) & #ff00) / 256    ; New track (25) for channel 3, as an offset. Offset MSB, then LSB.
    db ((Subsong0_Track25 - ($ + 1)) & 255)


; Position 11
    db 168    ; State byte.
    db ((Subsong0_Track26 - ($ + 2)) & #ff00) / 256    ; New track (26) for channel 1, as an offset. Offset MSB, then LSB.
    db ((Subsong0_Track26 - ($ + 1)) & 255)

    db ((Subsong0_Track27 - ($ + 2)) & #ff00) / 256    ; New track (27) for channel 2, as an offset. Offset MSB, then LSB.
    db ((Subsong0_Track27 - ($ + 1)) & 255)

    db ((Subsong0_Track28 - ($ + 2)) & #ff00) / 256    ; New track (28) for channel 3, as an offset. Offset MSB, then LSB.
    db ((Subsong0_Track28 - ($ + 1)) & 255)


; Position 12
    db 138    ; State byte.
    db 27    ; New height.
    db 128    ; New track (0) for channel 1, as a reference (index 0).
    db 128    ; New track (0) for channel 3, as a reference (index 0).

; Position 13
    db 170    ; State byte.
    db 65    ; New height.
    db ((Subsong0_Track29 - ($ + 2)) & #ff00) / 256    ; New track (29) for channel 1, as an offset. Offset MSB, then LSB.
    db ((Subsong0_Track29 - ($ + 1)) & 255)

    db ((Subsong0_Track30 - ($ + 2)) & #ff00) / 256    ; New track (30) for channel 2, as an offset. Offset MSB, then LSB.
    db ((Subsong0_Track30 - ($ + 1)) & 255)

    db ((Subsong0_Track31 - ($ + 2)) & #ff00) / 256    ; New track (31) for channel 3, as an offset. Offset MSB, then LSB.
    db ((Subsong0_Track31 - ($ + 1)) & 255)


; Position 14
Subsong0_Loop
    db 170    ; State byte.
    db 0    ; New height.
    db ((Subsong0_Track32 - ($ + 2)) & #ff00) / 256    ; New track (32) for channel 1, as an offset. Offset MSB, then LSB.
    db ((Subsong0_Track32 - ($ + 1)) & 255)

    db ((Subsong0_Track26 - ($ + 2)) & #ff00) / 256    ; New track (26) for channel 2, as an offset. Offset MSB, then LSB.
    db ((Subsong0_Track26 - ($ + 1)) & 255)

    db ((Subsong0_Track32 - ($ + 2)) & #ff00) / 256    ; New track (32) for channel 3, as an offset. Offset MSB, then LSB.
    db ((Subsong0_Track32 - ($ + 1)) & 255)


    db 1    ; End of the Song.
    db 0    ; Speed to 0, meaning "end of song".
Subsong0_DisarkByteRegionEnd2
Subsong0_DisarkPointerRegionStart3
    dw Subsong0_Loop

